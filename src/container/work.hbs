<div>
    {{#with main}}
    <main>
        <section>
            {{>title data = hobbies.title big = true}}
            <div>
                <ul>
                    {{#each work}}
                    <li>
                        <div>{{position}}</div>
                        <br>
                        <div>{{duration.from}} - {{duration.to}}</div>
                        <br>
                        <div>{{> link text=company.name href= company.url blank=true noLine=true}}</div>
                        <br><br>
                        <strong>Projects ({{projectAmount}})</strong>
                        <ul>
                            {{#each projects}}
                            <li>
                                <div>{{> link text=name href=url blank=true noLine=true}}</div>
                                <br>
                                <div>{{about}}</div>
                                <br>
                                <em>Projects stack ({{stackAmount}})</em>
                                <ul> <br>
                                    {{#each stack}}

                                    <li>
                                        <div>{{name}}</div>
                                    </li>
                                    {{/each}}
                                </ul>
                                <br>
                            </li>

                            <em>Projects awards ({{awardsAmount}})</em>
                            <br>
                            <ul>
                                {{#each awards}}
                                <br>
                                <li>
                                    <div>
                                        {{name}}
                                    </div>

                                    {{/each}}
                                </li>
                            </ul>
                            {{/each}}
                        </ul>

                    </li>
                    {{/each}}
                </ul>
            </div>
        </section>
    </main>
    {{/with}}
</div>